
public class Person {
	private String name ;
	private String xingbie ;	
	private int age ;
	private Boolean marry;
	public  String getname()
	{
		return name;
		}
	public String xingbie()
	{
		return xingbie;
	}
	public int age()
	{
		return age;
		}
	public Boolean marry()
	{
	if(age>=20)	
	{
		return true;
	}
	}

}
