
public class YH {
	public static void main(String agrs[]) 
	{
		 int[][] a=new int[10][10];
		 int i=0,j=0;
	for(i = 0;i < 10 ;i++)
	{
	    for(j = 0;j < i;j++)
	    {
	        if(i == j || j == 1) a[i][j] = 1;
	        else
	          a[i][j] = a[i][j-1] + a[i-1][j-1];
	        System.out.print(a[i][j]);
	     }
	     System.out.print('\n');
	}
	}
	}



